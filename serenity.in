#! /bin/bash

serenity_env_conf=("%CONFDIR%/serenity.conf"
                   "$HOME/.serenityrc"
                   "$PWD/serenity.conf"
                   )
serenity_env_lib="%LIBDIR%"
serenity_env_executable="$0"

if [ -f "${serenity_env_lib}/serenity.sh" ]; then
    . "${serenity_env_lib}/serenity.sh"
    serenity.main "$@"
fi
